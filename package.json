{
  "name": "ytubes",
  "version": "0.0.1",
  "description": "Search for videos, playlist, channel, movie and live on youtube without api key.",
  "author": "Valmisson Grizorte",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "lint": "eslint src/**/*.{js,ts}",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1 && git add CHANGELOG.md",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "lint-staged": "^11.2.0",
    "typescript": "^4.4.3",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "repository": "git@github.com:valmisson/ytubes.git",
  "bugs": "https://github.com/valmisson/ytubes/issues",
  "homepage": "https://github.com/valmisson/ytubes#readme",
  "keywords": [
    "youtube",
    "search",
    "youtube search",
    "yt",
    "ytube",
    "video",
    "channel",
    "music",
    "playlist",
